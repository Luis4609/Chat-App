{
    "name": "alrik11es/cowsayphp",
    "description": "Cowsay port in PHP",
    "keywords": [
        "cowsay"
    ],
    "homepage": "https://github.com/alrik11es/cowsayphp",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Sigueros",
            "email": "alrik11es@gmail.com",
            "homepage": "https://github.com/alrik11es",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Cowsayphp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cowsayphp\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "bin": [
        "bin/cowsayphp"
    ],
    "config":{
        "bin-dir": "bin"
    }
}
